{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gatherpress/venue-information",
  "version": "1.0.0",
  "title": "Venue Information",
  "category": "gatherpress",
  "icon": "location",
  "example": {},
  "description": "The venue information block.",
  "attributes": {
    "mapShow": {
      "type": "boolean",
      "default": true
    },
    "fullAddress": {
      "type": "string",
      "default": "currentLocation"
    },
    "phoneNumber": {
      "type": "string",
      "default": ""
    },
    "website": {
      "type": "string",
      "default": ""
    },
    "mapAlign": {
      "type": "string",
      "default": ""
    },
    "mapZoomLevel": {
      "type": "number",
      "default": 10
    },
    "mapType": {
      "type": "string",
      "default": "m"
    },
    "mapHeight": {
      "type": "number",
      "default": 300
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide"
    ]
  },
  "textdomain": "gatherpress",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./venue-information.js",
  "render": "file:./render.php"
}