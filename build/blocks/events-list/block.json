{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gatherpress/events-list",
  "version": "1.0.0",
  "title": "Events List",
  "category": "gatherpress",
  "icon": "list-view",
  "example": {},
  "description": "A block listing events.",
  "attributes": {
    "eventOptions": {
      "type": "object",
      "default": {
        "descriptionLimit": 55,
        "imageSize": "default",
        "showAttendeeList": true,
        "showFeaturedImage": true,
        "showDescription": true,
        "showRsvpButton": true,
        "showVenue": true
      }
    },
    "maxNumberOfEvents": {
      "type": "integer",
      "default": 5
    },
    "topics": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "venues": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "type": {
      "type": "string",
      "default": "upcoming"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "gatherpress",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "viewScript": "file:./events-list.js",
  "render": "file:./render.php"
}